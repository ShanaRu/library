21~30', '31~40', '41~50', '51~60', '>=70'],\r\n                axisTick: {\r\n                    alignWithLabel: true\r\n                }\r\n            }\r\n        ],\r\n        yAxis: [\r\n            {\r\n                type: 'value'\r\n            }\r\n        ],\r\n        series: [\r\n            {\r\n                name: '该年龄段的人数',\r\n                type: 'bar',\r\n                barWidth: '60%',\r\n                data: this.ageData\r\n            }\r\n    ]}\r\n        myChart.setOption(option)\r\n        window.addEventListener(\"resize\", function() {\r\n            myChart.resize()\r\n        })\r\n        }\r\n        this.$on('hook:destroyed',()=>{\r\n            window.removeEventListener(\"resize\", function() {\r\n                myChart.resize();\r\n            });\r\n        })\r\n                }\r\n                }).catch(error => {\r\n            this.$message({\r\n                type:'error',\r\n                message:'获取图书国家分类失败',\r\n                showClose:true\r\n            })\r\n        })\r\n\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n\r\n.card{\r\n  text-align: center;\r\n  margin-top: 1%;\r\n  line-height:1.7;\r\n  height: 16px;\r\n  \r\n}\r\n.box-card{\r\n  \r\n  width: 410px;\r\n}\r\n.team-card{\r\n  margin-top: 1%;\r\n  \r\n  \r\n  height:240px\r\n}\r\n.second{\r\n   text-align: center;\r\n   margin-top:9%;\r\n   line-height:1.7\r\n}\r\n.box-card-yuan{ \r\n  width: 200px;\r\n  border-radius:90%\r\n}\r\n.charts{\r\n text-align: center;\r\n margin-top: 1%;\r\n line-height:1.7;\r\n height: 20px;\r\n\r\n}\r\n\r\n\r\n.adminHomePage-container{\r\n   margin-top: 1%;\r\n   \r\n}\r\n\r\n.el-row {\r\n    margin-bottom: 18px;\r\n}\r\n\r\n.grid-content {\r\n    display: flex;\r\n    align-items: center;\r\n    height: 110px;\r\n}\r\n\r\n.grid-content2 {\r\n    margin-top: 15px;\r\n    display: flex;\r\n    align-items: center;\r\n    height: 45px;\r\n}\r\n\r\n.grid-cont-right {\r\n    flex: 1;\r\n    text-align